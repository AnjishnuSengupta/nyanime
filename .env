
# Jikan API doesn't require an API key, but we'll add support for it anyway
# You can leave this empty as Jikan is a free public API
VITE_JIKAN_API_KEY=

# YouTube Data API key for fetching trailer information (optional)
VITE_YOUTUBE_API_KEY=

# Video proxy URL for hiding the source of video requests
# This would point to your own proxy server in production
VITE_VIDEO_PROXY_URL=

# If you want to use other APIs in the future, add their keys here
# VITE_ANILIST_API_KEY=
# VITE_KITSU_API_KEY=
# VITE_TMDB_API_KEY=
